$ORIGIN PUB_3GPP_DOMAIN.
$TTL 1W
@                       1D IN SOA       localhost. root.localhost. (
                                        1		; serial
                                        3H              ; refresh
                                        15M             ; retry
                                        1W              ; expiry
                                        1D )            ; minimum

                        1D IN NS        pubns
pubns                   1D IN A         DNS_IP

aes                     1D IN A         ENTITLEMENT_SERVER_IP
